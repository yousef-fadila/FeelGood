<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apps.stisser.karissa.feelgood" >
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".FeelGood"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CountrySelection" android:screenOrientation="portrait" />
        <activity android:name=".DiscussionActivity" android:screenOrientation="portrait" />
        <activity android:name=".AskExpert" android:screenOrientation="portrait" />
        <activity android:name=".ExpertResponse" android:screenOrientation="portrait" />
    </application>

</manifest>
